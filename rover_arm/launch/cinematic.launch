<launch>

    <!--Lancer la node du controller-->
    <!-- <node name="arm_joy_node" pkg="joy" type="joy_node" > -->
         <!-- <param name="dev" type="string" value="/dev/input/" /> -->
    <!-- </node> -->

    <!-- Lancer la node de cinematiqe inverse-->
    <node name="arm_master" pkg="rover_arm" type="master_cpp" />

    <!--Lancer la communication avec l'arduino-->
    <node name="arm_arduino" pkg="rosserial_arduino" type="serial_node.py">
        <param name="port" value="/dev/ttyACM0"/>
    </node>

    <!--Lancer le service de calcul en cartÃ©sien-->
    <node name="arm_diff_kinetics_calc_server" pkg="rover_arm" type="diff_kinetics_calc_server.py" />

    <!--Lancer le gui rqt-->
    <node name = "arm_gui" pkg = "rqt_gui" type = "rqt_gui" respawn = "false" output = "screen"/>

</launch>
